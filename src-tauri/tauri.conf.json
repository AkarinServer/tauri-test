{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-riscv64-test",
  "version": "0.1.0",
  "identifier": "com.tauri.riscv64test",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Tauri RISCV64 Test",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["deb", "rpm"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/256x256.png",
      "icons/256x256@2x.png",
      "icons/512x512.png",
      "icons/512x512@2x.png"
    ]
  }
}

