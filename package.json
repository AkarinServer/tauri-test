{
  "name": "rv-verge",
  "version": "0.1.0",
  "description": "Clash Verge Rev - Lightweight version for RISC-V devices",
  "license": "GPL-3.0-only",
  "type": "module",
  "scripts": {
    "dev": "cross-env RUST_BACKTRACE=full tauri dev",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=2048' tauri build",
    "build:fast": "cross-env NODE_OPTIONS='--max-old-space-size=2048' tauri build -- --profile fast-release",
    "build:riscv64": "cross-env NODE_OPTIONS='--max-old-space-size=2048' tauri build -- --target riscv64gc-unknown-linux-gnu",
    "build:macos": "cross-env NODE_OPTIONS='--max-old-space-size=2048' tauri build -- --target aarch64-apple-darwin",
    "tauri": "tauri",
    "web:dev": "vite",
    "web:build": "tsc --noEmit && vite build",
    "web:serve": "vite preview",
    "prebuild": "node scripts/prebuild.mjs",
    "fmt": "cargo fmt --manifest-path ./src-tauri/Cargo.toml",
    "clippy": "cargo clippy --all-features --all-targets --manifest-path ./src-tauri/Cargo.toml",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@juggle/resize-observer": "^3.4.0",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@tauri-apps/api": "2.9.0",
    "@tauri-apps/plugin-clipboard-manager": "^2.3.2",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-fs": "^2.4.4",
    "@tauri-apps/plugin-http": "~2.5.4",
    "@tauri-apps/plugin-process": "^2.3.1",
    "@tauri-apps/plugin-shell": "2.3.3",
    "@tauri-apps/plugin-updater": "2.9.0",
    "@types/json-schema": "^7.0.15",
    "ahooks": "^3.9.6",
    "axios": "^1.13.2",
    "dayjs": "1.11.19",
    "foxact": "^0.2.49",
    "i18next": "^25.6.2",
    "js-yaml": "^4.1.0",
    "json-schema": "^0.4.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.1.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-error-boundary": "6.0.0",
    "react-hook-form": "^7.66.0",
    "react-i18next": "16.2.4",
    "react-router": "^7.9.5",
    "swr": "^2.3.6",
    "tauri-plugin-mihomo-api": "git+https://github.com/clash-verge-rev/tauri-plugin-mihomo",
    "types-pac": "^1.0.3"
  },
  "devDependencies": {
    "@tauri-apps/cli": "2.9.4",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.1",
    "@types/react": "19.2.3",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^4.2.1",
    "adm-zip": "^0.5.16",
    "cross-env": "^10.1.0",
    "glob": "^11.0.3",
    "https-proxy-agent": "^7.0.6",
    "node-fetch": "^3.3.2",
    "tar": "^7.5.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-svgr": "^4.5.0"
  }
}
